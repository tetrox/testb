{
   "$schema": "http://json-schema.org/draft-04/schema#",
   "type": "array",
   "items": {
     "type": "object",
     "required": [
       "id",
       "name"
     ],
     "properties": {
       "id": {
         "type": "number",
         "default": 0
       },
       "name": {
         "type": "string",
         "default": ""
       },
       "changedName": {
         "type": "string",
         "default": ""
       },
     "score": {
       "type": "string",
       "default": ""
     },
     "rate": {
       "type": "number",
       "default": 3,
       "minimum": 1,
       "maximum": 5
     }
   }
  }
}
